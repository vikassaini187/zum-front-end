            function call_script() {
//                alert(cityName);
                var org01 = $('#org').text();
//                alert(abc);
                if (org01 == "Orange County")
                {
                    window.open("https://www.onboardiq.com/zum/apply/zumer-driver-for-kids");

                }
                else
                {
                    window.open("https://www.onboardiq.com/zum/apply/zum-applicants");
                }

            }



